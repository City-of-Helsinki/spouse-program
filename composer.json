{
    "name": "druidfi/puoliso-ohjelma",
    "type": "project",
    "description": "Puoliso-ohjelma",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Druid.fi",
            "email": "info@druid.fi"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "johnpbloch/wordpress": "^5.4",
        "wpackagist-plugin/contact-form-7": "^5.1",
        "wpackagist-plugin/google-sitemap-generator": "^4.1",
        "wpackagist-plugin/lumturio-wp-monitor": "^1.0",
        "wpackagist-plugin/site-health-tool-manager": "^1.1",
        "wpackagist-plugin/mobile-menu": "^2.8",
        "wpackagist-plugin/flamingo": "^2.1",
        "wpackagist-plugin/modal-window":"5.0.5",
        "wpackagist-plugin/advanced-custom-fields":"5.8.11",
        "wpackagist-plugin/disable-gutenberg":"2.1",
        "wpackagist-plugin/import-eventbrite-events":"1.5.8",
        "wpackagist-plugin/all-in-one-wp-migration":"7.22",
        "wpackagist-plugin/my-calendar":"3.1.18",
        "wpackagist-plugin/wordpress-importer":"0.7"
    },
    "require-dev": {
        "wp-cli/wp-cli": "^2.4"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "installer-paths": {
            "public/wp-content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "public/wp-content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        },
        "preserve-paths": [
            "public/wp-config.php",
            "public/wp-content/uploads"
        ],
        "wordpress-install-dir": "public"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
